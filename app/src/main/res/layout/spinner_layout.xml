<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/_60"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardElevation="0dp"
    app:cardBackgroundColor="@color/white"
    app:cardCornerRadius="@dimen/h1"
    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/h2"
            android:layout_marginEnd="@dimen/h2"
            android:layout_marginTop="@dimen/h5"
            android:layout_marginBottom="@dimen/h5"
            android:gravity="center_vertical"
            android:singleLine="true"
            android:paddingStart="@dimen/h2"
            android:paddingEnd="@dimen/h2"
            android:textColor="@color/black"
            android:textSize="@dimen/text_h3"
            android:id="@+id/textNama"
            android:layout_alignParentStart="true"
            android:ellipsize="end"
            />
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/gray2"
            android:layout_alignParentBottom="true"
            />
    </RelativeLayout>
</androidx.cardview.widget.CardView>